         .MACRO MOVE.L 
         .IF %3<$0100
           LDX #%3-1
@MC        LDA %1,X
           STA %2,X
           DEX 
           .IF %3>$7F
             CPX #$FF
             BNE @MC
             .ELSE 
             BPL @MC
             .ENDIF 
           .ENDIF 
         .IF %3&$FF<>$00
           .IF %3>$0100
             LDA #$00
             STA LI
             STA LI+1
?MO          LDA %1
?MO1         STA %2
             INC ?MO+1
             BNE ?MO2
             INC ?MO+2
?MO2         INC ?MO1+1
             BNE ?MO3
             INC ?MO1+2
?MO3         INC LI
             BNE ?MO4
             INC LI+1
?MO4         LDA LI+1
             CMP # >%3
             BNE ?MO
             LDA LI
             CMP # <%3
             BNE ?MO
             LDA #$00
             STA LI
             STA LI+1
             BEQ ?MO5
LI           .WORD 0
?MO5
             .ENDIF 
           .ELSE 
           LDX # >%3
           LDY #$00
?MO        LDA %1,Y
?MO1       STA %2,Y
           INY 
           BNE ?MO
           INC ?MO+2
           INC ?MO1+2
           DEX 
           BNE ?MO
           .ENDIF 
         .ENDM 
         .MACRO VDEC 
         DEC %1
         LDA %1
         CMP #$FF
         BNE VD1
         DEC %1+1
VD1
         .ENDM 
