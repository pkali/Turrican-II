         .MACRO ADD.W 
         LDA %1
         CLC 
         ADC %2
         .IF %0=3
           STA %3
           .ELSE 
           STA %2
           .ENDIF 
         LDA %1+1
         ADC %2+1
         .IF %0=3
           STA %3+1
           .ELSE 
           STA %2+1
           .ENDIF 
         .ENDM 
;
         .MACRO ADD.Q 
         .IF %2>$FF
           LDA %1
           CLC 
           ADC # <%2
           STA %1
           LDA %1+1
           ADC # >%2
           STA %1+1
           .ELSE 
           LDA %1
           CLC 
           ADC #%2
           STA %1
           BCC ADQ0
           INC %1+1
ADQ0
           .ENDIF 
         .ENDM 
