         .MACRO FILL 
          DPOKE  ?FL+1,%1
         LDY #$00
         LDX # >[%2-%1]-$0100
         LDA #%3
?FL      STA %1,Y
         INY 
         BNE ?FL
         INC ?FL+2
         DEX 
         BPL ?FL
         .ENDM 
         .MACRO VCMP 
         LDA %1+1
         CMP # >%2
         BNE %3
         LDA %1
         CMP # <%2
         BNE %3
         .ENDM 
